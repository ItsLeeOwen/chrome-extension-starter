  {
    "name": "chrome-extension-starter",
    "version": "0.0.1",
    "description": "Chrome extension starter",
    "manifest_version": 2,
    "browser_action": {
      "default_title": "Chrome Extension Starter",
      "default_popup": "popup.html"
    },
    "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----",
    "oauth2": {
      "client_id": "736837297898-5j95h1pfeji56bo07t0qng1a20kb52lo.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.profile",
        "https://www.googleapis.com/auth/userinfo.email"
      ]
    },
    "permissions": [
      "activeTab",
      "identity",
      "management",
      "storage"
    ],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }